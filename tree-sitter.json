{
  "grammars": [
    {
      "name": "cython",
      "camelcase": "Cython",
      "scope": "source.cython",
      "path": ".",
      "file-types": [
        "pyx",
        "pxd"
      ],
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm",
      "locals": "queries/locals.scm",
      "tags": "queries/tags.scm"
    }
  ],
  "metadata": {
    "version": "1.2.0",
    "license": "MIT",
    "description": "cython grammar for tree-sitter",
    "links": {
      "repository": "https://github.com/tree-sitter-grammars/tree-sitter-python"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}